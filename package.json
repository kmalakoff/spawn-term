{
  "name": "spawn-term",
  "version": "0.0.5",
  "description": "Formats spawn with for terminal grouping",
  "keywords": [
    "spawn",
    "cross-spawn",
    "cross-spawn-cb",
    "streaming",
    "format",
    "terminal",
    "groups",
    "grouping",
    "group"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/spawn-term.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "import": "./dist/esm/index.mjs",
