    "readable-stream": "^2.3.8"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*",
    "@types/react": "*",
    "cr": "*",
    "is-version": "*",
    "pinkie-promise": "*"
  },
  "engines": {
    "node": ">=16"
