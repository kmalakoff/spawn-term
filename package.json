{
  "name": "spawn-term",
  "version": "3.3.3",
  "description": "Formats spawn with for terminal grouping",
  "keywords": [
    "spawn",
    "cross-spawn",
    "cross-spawn-cb",
    "streaming",
    "format",
    "terminal",
    "groups",
    "grouping",
    "group"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/spawn-term.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/cjs/src/index-cjs.d.ts",
      "import": "./dist/esm/index-esm.js",
      "require": "./dist/cjs/index-cjs.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index-cjs.js",
  "types": "dist/cjs/src/index-cjs.d.ts",
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "tsds format",
    "prepublishOnly": "tsds validate",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines tsds test:node --no-timeouts",
    "version": "tsds version"
  },
  "dependencies": {
    "@xterm/headless": "^5.5.0",
    "cross-spawn-cb": "^2.4.12",
    "install-module-linked": "^1.3.14",
    "on-one": "^1.0.8",
    "queue-cb": "^1.6.1"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.1",
    "@types/react": "^19.2.7",
    "cr": "^0.1.0",
    "ink": "^6.5.1",
    "is-version": "^1.0.7",
    "node-version-use": "^2.1.5",
    "pinkie-promise": "^2.0.1",
    "react": "^19.2.3",
    "ts-dev-stack": "^1.21.3",
    "tsds-config": "^0.2.1"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index-esm.ts"
  }
}
