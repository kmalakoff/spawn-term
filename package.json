{
  "name": "spawn-term",
  "version": "0.1.52",
  "description": "Formats spawn with for terminal grouping",
  "keywords": [
    "spawn",
    "cross-spawn",
    "cross-spawn-cb",
    "streaming",
    "format",
    "terminal",
    "groups",
    "grouping",
    "group"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/spawn-term.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index-esm.js",
      "require": "./dist/cjs/index-cjs.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index-cjs.js",
  "types": "dist/cjs/index-cjs.d.cts",
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "core-js": "^3.42.0",
    "cross-spawn-cb": "^2.2.1",
    "lil-uuid": "^0.1.1",
    "on-one": "^0.1.6",
    "queue-cb": "^1.5.0",
    "react": "^18.3.1",
    "readable-stream": "^2.3.8",
    "yoga-wasm-web": "^0.3.3",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*",
    "@types/react": "*",
    "cr": "*",
    "is-version": "*",
    "pinkie-promise": "*"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index-esm.ts"
  }
}
