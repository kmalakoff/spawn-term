{
  "compilerOptions": {
    "skipLibCheck": true,
    "lib": ["ES5"],
    "target": "ES5",
    "module": "CommonJS",
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "allowJs": true
  }
}
